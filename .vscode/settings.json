{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.validate": ["json"],
  "typescript.tsdk": "node_modules/typescript/lib",
  "cSpell.words": [
    "autoload",
    "execa",
    "hostssl",
    "initdb",
    "pmmmwh",
    "revparse",
    "tmpfs",
    "zenrows"
  ],
  "cSpell.ignorePaths": [
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "pnpm-lock.yaml",
    ".nx",
    "tmp"
  ],
  "eslint.experimental.useFlatConfig": true
}
