{
  "name": "tw-erp-next",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "tw/erp/next",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/tw/erp/next"
      },
      "configurations": {
        "development": {
          "outputPath": "tw/erp/next"
        },
        "production": {}
      },
      "dependsOn": ["build-custom-server"]
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "tw-erp-next:build",
        "dev": true,
        "customServerTarget": "tw-erp-next:serve-custom-server"
      },
      "configurations": {
        "development": {
          "buildTarget": "tw-erp-next:build:development",
          "dev": true,
          "customServerTarget": "tw-erp-next:serve-custom-server:development"
        },
        "production": {
          "buildTarget": "tw-erp-next:build:production",
          "dev": false,
          "customServerTarget": "tw-erp-next:serve-custom-server:production"
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "tw-erp-next:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "tw/erp/next/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "build-custom-server": {
      "executor": "@nx/js:swc",
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/tw/erp/next",
        "main": "tw/erp/next/server/main.ts",
        "tsConfig": "tw/erp/next/tsconfig.server.json",
        "clean": false,
        "assets": []
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve-custom-server": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "tw-erp-next:build-custom-server"
      },
      "configurations": {
        "development": {
          "buildTarget": "tw-erp-next:build-custom-server:development"
        },
        "production": {
          "buildTarget": "tw-erp-next:build-custom-server:production"
        }
      }
    }
  },
  "tags": []
}
