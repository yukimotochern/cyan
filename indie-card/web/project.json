{
  "name": "indie-card-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "indie-card/web",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "outputPath": "dist/indie-card/web"
      },
      "configurations": {
        "development": {
          "outputPath": "indie-card/web"
        },
        "production": {
          "generateLockfile": true
        }
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "indie-card-web:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "indie-card-web:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "indie-card-web:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "indie-card-web:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "indie-card/web/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["indie-card/web/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
